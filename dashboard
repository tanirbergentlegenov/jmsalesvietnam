<script>
/**
 * CONFIG â€“ YOUR REAL LIVE SHEET LINKS
 */
const CONFIG = {
  prmSummaryCsvUrl: "https://docs.google.com/spreadsheets/d/1beV69dn9_XvvQ8fEmOMTfE_Lpb97XmA3/gviz/tq?tqx=out:csv&sheet=PRM_Summary",
  ibCsvUrl: "https://docs.google.com/spreadsheets/d/1beV69dn9_XvvQ8fEmOMTfE_Lpb97XmA3/gviz/tq?tqx=out:csv&sheet=IB_List",
  tasksCsvUrl: "https://docs.google.com/spreadsheets/d/1beV69dn9_XvvQ8fEmOMTfE_Lpb97XmA3/gviz/tq?tqx=out:csv&sheet=Tasks"
};

// Static player config (avatars / Naruto roles)
const PLAYERS = [
  {
    id: "Tri Vu",
    nick: "Gaara",
    level: 7,
    baseScore: 0,         // will be increased by ticket points
    avatarText: "Gaara\nplaceholder",
  },
  {
    id: "Khiem",
    nick: "Shikamaru",
    level: 6,
    baseScore: 0,
    avatarText: "Shikamaru\nplaceholder",
  }
];

/**
 * UTIL: basic CSV fetcher (no external libraries)
 */
async function fetchCsv(url) {
  const res = await fetch(url);
  if (!res.ok) throw new Error("Failed to load " + url);
  const text = await res.text();
  const [headerLine, ...lines] = text.trim().split(/\r?\n/);
  const headers = headerLine.split(",").map(h => h.trim());
  return lines.filter(Boolean).map(line => {
    const cells = line.split(","); // simple split â€“ avoid commas inside values in your sheet
    const obj = {};
    headers.forEach((h, i) => { obj[h] = (cells[i] || "").trim(); });
    return obj;
  });
}

/**
 * RENDER: Players strip (top row)
 */
function renderPlayers(prmSummaryRows, tasksRows) {
  const container = document.getElementById("playersRow");
  container.innerHTML = "";

  PLAYERS.forEach(player => {
    const summary = prmSummaryRows.find(r => r.PRM === player.id) || {};
    const tickets = tasksRows.filter(t => t.PRM === player.id);
    const donePoints = tickets
      .filter(t => (t.Status || "").toLowerCase() === "done")
      .reduce((sum, t) => sum + (parseFloat(t.Points) || 0), 0);

    const score = player.baseScore + donePoints;

    const newPartners = parseInt(summary.NewActivePartners || "0", 10);
    const t1 = 4, t2 = 6, t3 = 8;
    let partnerTier = 0;
    if (newPartners >= t3) partnerTier = 3;
    else if (newPartners >= t2) partnerTier = 2;
    else if (newPartners >= t1) partnerTier = 1;

    const prevVolume = parseFloat(summary.PrevVolume || "0");
    const currentVolume = parseFloat(summary.CurrentVolume || "0");
    const nominalY = parseFloat(summary.YNominal || "0");

    const threshold1 = prevVolume * 1.05 + nominalY * t1;
    const threshold2 = prevVolume * 1.10 + nominalY * t2;
    const threshold3 = prevVolume * 1.15 + nominalY * t3;

    let volumeTier = 0;
    if (currentVolume >= threshold3) volumeTier = 3;
    else if (currentVolume >= threshold2) volumeTier = 2;
    else if (currentVolume >= threshold1) volumeTier = 1;

    const partnerProgress =
      partnerTier === 0 ? (newPartners / t1) * 100 :
      partnerTier === 1 ? ((newPartners - t1) / (t2 - t1)) * 100 :
      partnerTier === 2 ? ((newPartners - t2) / (t3 - t2)) * 100 : 100;

    const volumeTargetForTier =
      volumeTier === 0 ? threshold1 :
      volumeTier === 1 ? threshold2 :
      volumeTier === 2 ? threshold3 : currentVolume;

    const volumeProgress = volumeTargetForTier
      ? Math.min(100, (currentVolume / volumeTargetForTier) * 100)
      : 0;

    const card = document.createElement("div");
    card.className = "player-card";
    card.innerHTML = `
      <div class="player-avatar">
        <!-- Replace this with <img src="gaara.png" alt=""> if you have images -->
        <span>${player.avatarText}</span>
      </div>
      <div class="player-main">
        <div class="player-header">
          <div class="player-name">
            ${player.id}
            <small>${player.nick}</small>
          </div>
          <div class="points-pill">${score.toFixed(0)} pts</div>
        </div>
        <div class="player-meta">
          <span><span class="dot"></span>Level ${player.level}</span>
          <span>ðŸŽ¯ New partners: <strong>${newPartners || 0}</strong></span>
          <span>ðŸ’° Volume: <strong>${(currentVolume || 0).toLocaleString()}</strong> USD</span>
        </div>

        <div class="player-progress">
          <div>
            <div class="progress-bar">
              <div class="progress-fill"
                   style="width:${Math.max(6, Math.min(100, partnerProgress || 0))}%"></div>
            </div>
          </div>
          <div class="progress-label">
            Partner Tier: ${partnerTier}/3
          </div>
        </div>

        <div class="player-progress" style="margin-top:4px;">
          <div>
            <div class="progress-bar">
              <div class="progress-fill"
                   style="width:${Math.max(6, Math.min(100, volumeProgress || 0))}%"></div>
            </div>
          </div>
          <div class="progress-label">
            Volume Tier: ${volumeTier}/3
          </div>
        </div>

        <div class="kpi-strip">
          <div class="kpi-chip">
            <span class="kpi-label">Target 1</span>
            <span class="kpi-value">${t1} partners</span>
            <span class="kpi-state ${partnerTier >= 1 ? "kpi-state--ok" : "kpi-state--warn"}">
              ${partnerTier >= 1 ? "Hit" : "Pending"}
            </span>
          </div>
          <div class="kpi-chip">
            <span class="kpi-label">Target 2</span>
            <span class="kpi-value">${t2} partners</span>
            <span class="kpi-state ${partnerTier >= 2 ? "kpi-state--ok" : "kpi-state--warn"}">
              ${partnerTier >= 2 ? "Hit" : "Pending"}
            </span>
          </div>
          <div class="kpi-chip">
            <span class="kpi-label">Target 3</span>
            <span class="kpi-value">${t3} partners</span>
            <span class="kpi-state ${partnerTier >= 3 ? "kpi-state--ok" : "kpi-state--bad"}">
              ${partnerTier >= 3 ? "Hit" : "Stretch"}
            </span>
          </div>
        </div>
      </div>
    `;
    container.appendChild(card);
  });
}

/**
 * RENDER: IB tables
 */
function renderIbTables(ibRows) {
  const container = document.getElementById("ibTables");
  container.innerHTML = "";

  PLAYERS.forEach(player => {
    const ibForPrm = ibRows.filter(r => r.PRM === player.id);

    const block = document.createElement("div");
    block.style.marginBottom = "14px";
    block.innerHTML = `
      <h3 style="font-size:13px;font-weight:600;margin:6px 0 4px;">
        ${player.id}'s New IBs
        <span class="muted">(${ibForPrm.length} partners)</span>
      </h3>
      <div style="overflow-x:auto;border-radius:10px;border:1px solid rgba(30,41,59,0.8);">
        <table>
          <thead>
            <tr>
              <th>#</th>
              <th>IB Name</th>
              <th>IB ID</th>
              <th>Active Clients</th>
              <th>Trading Volume, USD</th>
            </tr>
          </thead>
          <tbody>
            ${ibForPrm.map((ib, idx) => `
              <tr>
                <td>${idx + 1}</td>
                <td>${ib.IBName || ""}</td>
                <td>${ib.IBID || ""}</td>
                <td>${ib.ActiveClients || 0}</td>
                <td>${(parseFloat(ib.TradingVolume || "0")).toLocaleString()}</td>
              </tr>
            `).join("")}
          </tbody>
        </table>
      </div>
    `;
    container.appendChild(block);
  });
}

/**
 * RENDER: KPI overview (whole team)
 */
function renderKpiOverview(prmSummaryRows) {
  const container = document.getElementById("kpiContainer");
  container.innerHTML = "";

  const totalPartners = prmSummaryRows.reduce(
    (sum, r) => sum + (parseInt(r.NewActivePartners || "0", 10)), 0);
  const prevVolume = prmSummaryRows.reduce(
    (sum, r) => sum + (parseFloat(r.PrevVolume || "0")), 0);
  const currentVolume = prmSummaryRows.reduce(
    (sum, r) => sum + (parseFloat(r.CurrentVolume || "0")), 0);

  const t1 = 4, t2 = 6, t3 = 8;
  const partnerRate = totalPartners;

  const yTotal = prmSummaryRows.reduce(
    (sum, r) => sum + (parseFloat(r.YNominal || "0")), 0);

  const volumeT1 = prevVolume * 1.05 + yTotal * t1;
  const volumeT2 = prevVolume * 1.10 + yTotal * t2;
  const volumeT3 = prevVolume * 1.15 + yTotal * t3;

  const block = document.createElement("div");
  block.innerHTML = `
    <div class="kpi-strip" style="margin-top:4px;">
      <div class="kpi-chip">
        <span class="kpi-label">New Active Partners</span>
        <span class="kpi-value">${partnerRate} / ${t3}</span>
        <span class="kpi-state ${partnerRate >= t3 ? "kpi-state--ok" : partnerRate >= t1 ? "kpi-state--warn" : "kpi-state--bad"}">
          ${partnerRate >= t3 ? "Target 3" : partnerRate >= t2 ? "Target 2" : partnerRate >= t1 ? "Target 1" : "Below"}
        </span>
      </div>

      <div class="kpi-chip">
        <span class="kpi-label">Volume vs T2</span>
        <span class="kpi-value">
          ${(currentVolume/1_000).toFixed(1)}k / ${(volumeT2/1_000).toFixed(1)}k
        </span>
        <span class="kpi-state ${currentVolume >= volumeT2 ? "kpi-state--ok" : "kpi-state--warn"}">
          ${currentVolume >= volumeT2 ? "On track" : "Push"}
        </span>
      </div>

      <div class="kpi-chip">
        <span class="kpi-label">Growth vs Last Month</span>
        <span class="kpi-value">
          ${prevVolume ? (((currentVolume - prevVolume) / prevVolume) * 100).toFixed(1) + "%" : "â€”"}
        </span>
        <span class="kpi-state ${currentVolume >= volumeT1 ? "kpi-state--ok" : "kpi-state--bad"}">
          ${currentVolume >= volumeT1 ? "Above T1" : "Below T1"}
        </span>
      </div>
    </div>
  `;
  container.appendChild(block);
}

/**
 * RENDER: Tasks / backlog tables
 */
function renderTasks(tasksRows) {
  const container = document.getElementById("tasksContainer");
  container.innerHTML = "";

  PLAYERS.forEach(player => {
    const tickets = tasksRows.filter(t => t.PRM === player.id);

    const block = document.createElement("div");
    block.style.marginBottom = "12px";
    block.innerHTML = `
      <h3 style="font-size:13px;font-weight:600;margin:6px 0 4px;">
        ${player.id}'s Missions
        <span class="muted">(${tickets.length} tickets)</span>
      </h3>
      <div style="overflow-x:auto;border-radius:10px;border:1px solid rgba(30,41,59,0.8);">
        <table>
          <thead>
            <tr>
              <th>ID</th>
              <th>Task</th>
              <th>Status</th>
              <th>Points</th>
            </tr>
          </thead>
          <tbody>
            ${tickets.map(t => {
              const status = (t.Status || "").toLowerCase();
              const tagClass =
                status === "done" ? "tag-done" :
                status === "in progress" ? "tag-progress" : "tag-backlog";
              const dotClass =
                status === "done" ? "done" :
                status === "in progress" ? "progress" : "backlog";
              return `
                <tr>
                  <td>${t.TicketID || ""}</td>
                  <td>${t.Title || ""}</td>
                  <td>
                    <span class="tag ${tagClass}">
                      <span class="status-dot ${dotClass}"></span>${t.Status || "Backlog"}
                    </span>
                  </td>
                  <td>${t.Points || 0}</td>
                </tr>`;
            }).join("")}
          </tbody>
        </table>
      </div>
    `;
    container.appendChild(block);
  });
}

/**
 * MAIN: load all sheets and render dashboard
 */
async function initDashboard() {
  try {
    const [prmSummary, ibRows, tasksRows] = await Promise.all([
      fetchCsv(CONFIG.prmSummaryCsvUrl),
      fetchCsv(CONFIG.ibCsvUrl),
      fetchCsv(CONFIG.tasksCsvUrl)
    ]);

    renderPlayers(prmSummary, tasksRows);
    renderIbTables(ibRows);
    renderKpiOverview(prmSummary);
    renderTasks(tasksRows);
  } catch (err) {
    console.error(err);
    alert("Error loading data from the sheet. Check the CSV links in CONFIG and sharing settings (Anyone with link â€“ Viewer).");
  }
}

initDashboard();
</script>
</body>
</html>
